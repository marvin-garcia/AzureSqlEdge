{
    "content": {
      "modulesContent": {
        "$edgeAgent": {
          "properties.desired.modules.simTempSensor": {
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
              "createOptions": ""
            },
            "type": "docker",
            "env": {
              "MessageCount": {
                "value": "-1"
              },
              "SendInterval": {
                  "value": "30"
              }
            },
            "status": "running",
            "restartPolicy": "always",
            "version": "1.0"
          }
        },
        "$edgeHub": {
          "properties.desired.routes.simTempSensorToSqlEdge": "FROM /messages/modules/simTempSensor/* INTO BrokeredEndpoint(\"modules/sqlEdge/inputs/sensorinput\")"
        },
        "simTempSensor": {
          "properties.desired": {
            "SendData": true
          }
        }
      }
    }
  }